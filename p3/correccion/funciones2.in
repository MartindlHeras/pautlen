main
{

    int v1, v2, resultado;


    function int fibonacci (int num1)
    {

	int res1, res2;
	

	if ((num1 > 0))
	{
	    if ((num1 == 0))
	    {
		return 0;
	    }  
	    else 
	    {
		if ((num1 == 1)) 
		{
		    return 1;
		}
		else {
		    res1 = fibonacci (num1 - 1);
		    res2 = fibonacci (num1 - 2);
		    return res1 + res2;
		}
	    }
		
	}
    }

// Sentencias

	scanf v1;

	resultado = fibonacci (v1);

	printf resultado;
}





